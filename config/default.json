{
    "app": {
      "name": "Progress Tracker API",
      "version": "1.0.0"
    },
    "cors": {
      "origin": "*",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    "rateLimiter": {
      "windowMs": 900000,
      "max": 100
    },
    "logging": {
      "level": "info",
      "maxFiles": "14d"
    }
  }
  